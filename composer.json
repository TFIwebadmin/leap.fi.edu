{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "dev-master",
        "drupal/address": "^1.4",
        "drupal/admin_toolbar": "^1.24",
        "drupal/auto_entitylabel": "^2.1",
        "drupal/better_exposed_filters": "^3.0",
        "drupal/block_class": "^1.0@alpha",
        "drupal/block_content_permissions": "^1.6",
        "drupal/console": "^1.0.2",
        "drupal/contribute": "^1.0",
        "drupal/core": "^8.6.0",
        "drupal/ctools": "^3.0",
        "drupal/discourse_sso": "^1.0",
        "drupal/entityqueue": "^1.0",
        "drupal/feeds": "^3.0@alpha",
        "drupal/field_collection": "^1.0@alpha",
        "drupal/field_group": "^3.0",
        "drupal/geocoder": "^2.0",
        "drupal/geofield": "^1.0",
        "drupal/geofield_map": "^2.3",
        "drupal/google_analytics": "^2.2",
        "drupal/honeypot": "^1.27",
        "drupal/masquerade": "^2.0@beta",
        "drupal/me_redirect": "^1.0@beta",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/metatag": "^1.5",
        "drupal/module_filter": "^3.1",
        "drupal/pathauto": "^1.2",
        "drupal/permissions_by_term": "^2.6",
        "drupal/roleassign": "^1.0",
        "drupal/smtp": "^1.0@beta",
        "drupal/video_embed_field": "^2.0",
        "drupal/webform": "^5.0",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        }
    }
}
